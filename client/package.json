{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "enzyme": "^2.9.1",
    "material-ui": "^0.18.7",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.6",
    "react-redux": "^5.0.5",
    "react-reveal": "^0.1.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0"
  },
  "proxy": "http://localhost:8000/",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/stylesheet --include-path ./node_modules src/stylesheet/ -o src/stylesheet/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/stylesheet --include-path ./node_modules src/stylesheet/ -o src/stylesheet/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
